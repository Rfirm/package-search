<!DOCTYPE html>
<html>
  <title>
    {{title}}
  </title>
  <head>
    <meta property="og:title" content="{{og_title}}">
    <meta property="og:site_name" content="{{og_site_name}}">
    <meta property="og:url" content="{{og_url}}">
    <meta property="og:description" content="{{og_description}}">
    <meta property="og:image" content="">
    <meta name="author" content="{{og_author}}">
    <meta charset="{{charset}}">
    <meta name="viewport" content="{{viewport}}">
    <link rel="shortcut icon" href="{{shortcut_icon}}">
    {{#each stylesheets}}
    <link rel="stylesheet" href="{{this}}">
    {{/each}}
    {{#each scripts}}
    <script src="{{this}}"></script>
    {{/each}}
  </head>
  <body>
    <div class="row">
      <div class="usage-page">
        <div class="wrapper">
          <header id="header">
            <nav id="navigation" role="navigation">
              <div class="wrapper"><a href="./" class="logo"><h2>CRAN package search engine</h2></a>
                <ul>
                  <li><a href="{{github_link}}" target="_blank">Github</a></li>
                </ul>
              </div>
            </nav>
          </header>
          <main id="pkg-list">
            <div class="col-md-10 ">
              <div id="page_area">
              </div>
              <div id="list_data">
              </div>
            </div>
          </main>
        </div>

      </div>
    </div>
    <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
      
      ga('create', '{{google_analytics.UA}}', '{{google_analytics.url}}');
      ga('send', 'pageview');
    </script>
    <script data-main="scripts/app.js" src="//cdnjs.cloudflare.com/ajax/libs/require.js/2.1.11/require.min.js"></script>
  </body>
</html>
